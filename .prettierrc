{
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": false,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
  "jsxBracketSameLine": false,
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
	"arrowParens": "always",
	"rangeStart": 0,
	"parser": "babel",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve"
}
